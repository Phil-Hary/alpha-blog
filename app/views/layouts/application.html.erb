<!DOCTYPE html>
<html>
  <head>
    <title>AlphaBlog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body id="home-body">
	
		<%= render 'layouts/navigations' %>
		<% flash.each do |name,msg| %>
			<%= msg %>
		<% end %>
    <%= yield %>
  </body>
</html>
